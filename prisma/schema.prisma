datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Experiment {
  id          Int      @id @default(autoincrement())
  prompt      String
  temperature Float
  topP        Float
  response    String
  metrics     Json
  createdAt   DateTime @default(now())
}
